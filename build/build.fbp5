              <Object Class="TBuildScript" XMLID="{D5D3BEF2-1E7F-42DA-ADC1-5B029CFF769A}"><Property MultiValue="0" name="BreakPointsStore" type="string"></Property>
                <Property MultiValue="0" name="Name" type="string">OnExecute</Property>
                <Property MultiValue="1" name="Script" type="string"><![CDATA[   // dxGetTextPath;
   
   // We assume dxgettext is in the system path variable, and "dxgettext" is in the name.
   // This is *far* from perfect, but sufficient for now
   path_arr = Path.split(';');
   for(i in path_arr)
   {
       if (/dxgettext/.test(path_arr[i]))
         dxGetTextPath = path_arr[i];
   }
   
   if (dxGetTextPath != '')
     ActionResult = true;
   else {
     SendLogMessage('Failed to determine dxgettext path - cannot continue.');
     ActionResult = false;
   }
]]>
                  </Property>
                <Property MultiValue="0" name="ScriptLanguage" type="string">JavaScript</Property>
                </Object>
              </Object>
            </Object>
