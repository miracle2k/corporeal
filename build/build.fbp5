              <Property MultiValue="1" name="Script" type="string"><![CDATA[   // dxGetTextPath;
   
   // We assume dxgettext is in the system path variable, and "dxgettext" is in the name.
   // This is *far* from perfect, but sufficient for now
   path_arr = Path.split(';');
   for(i in path_arr)
   {
       if (/dxgettext/.test(path_arr[i]))
         dxGetTextPath = path_arr[i];
   }
   
   if (dxGetTextPath != '')
     ActionResult = true;
   else {
     SendLogMessage('Failed to determine dxgettext path - cannot continue.');
     ActionResult = false;
   }
]]>
