    <Property MultiValue="1" name="GlobalJScript" type="string"><![CDATA[    // ******************************************
    // Version number stuff
    var MinorVersionDigits = 2;

    function FormatMinor(n)
    {
        n = n.toString();
        // add padding
        for (i=0; i &lt; (MinorVersionDigits-n.length); i++) n = '0'+n;
        // remove trailing zeros
        n = n.replace(/0+$/, '');
        // if string is empty now, set to "0"
        if (n=='') n = '0';
        // return
        return n;
    }

    // This should be called once the version numbers are intialized,
    // and everytime they are changed (e.g. when the build numer is
    // incremented).
    function UpdateVersionStringVariables()
    {
        VersionStrMinor = FormatMinor(VersionMinor);
        VersionStrShort = VersionMajor+'.'+VersionStrMinor;
        VersionStrLong = VersionStrShort;
        if (VersionIdent!='') VersionStrLong += '-'+VersionIdent;
        VersionStrFull = VersionStrLong;
        if (VersionBuild!=0) VersionStrFull += '.'+VersionBuild;
    }

    // ******************************************
]]>
      </Property>
